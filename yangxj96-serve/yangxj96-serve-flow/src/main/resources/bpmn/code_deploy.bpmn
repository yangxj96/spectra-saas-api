        <bpmn2:sequenceFlow sourceRef="start" targetRef="audit"/>
        <bpmn2:sequenceFlow sourceRef="audit" targetRef="end"/>
    </bpmn2:process>
    <bpmndi:BPMNDiagram id="BPMNDiagram_code_deploy">
        <bpmndi:BPMNPlane bpmnElement="code_deploy" id="BPMNPlane_code_deploy"/>
    </bpmndi:BPMNDiagram>
